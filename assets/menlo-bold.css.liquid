@font-face {font-family: "Menlo Bold";
  src: url({{ 'menlo-bold.eoteot' | asset_url }}); /* IE9*/
  src: url({{ 'menlo-bold.eoteot?#iefix' | asset_url }}) format("embedded-opentype"), /* IE6-IE8 */
  url({{ 'menlo-bold.woff2' | asset_url }}) format("woff2"), /* chrome、firefox */
  url({{ 'menlo-bold.woff' | asset_url }}) format("woff"), /* chrome、firefox */
  url({{ 'menlo-bold.ttf' | asset_url }}) format("truetype"), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
  url({{ 'menlo-bold..svg#Menlo Bold' | asset_url }}) format("svg"); /* iOS 4.1- */
}
